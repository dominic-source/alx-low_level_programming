#include <stdlib.h>
#include <time.h>
#include <stdio.h>

/**
 * main -  Display a string.
 * Description: Display a string that has a random number and the last digit of the random number
 * Return: it returns zero
 */
int main(void)
{
  int n;

  srand(time(0));
  n = rand() - RAND_MAX / 2;
  printf("Last digit of %i is %i \n",n, (int)sizeof(n));
  return (0);
}
